    <script>
        function blockuser(userId){
            console.log(userId)
         Swal.fire({
       title: 'Are you sure?',
       text: "You are Blocking a User!",
       icon: 'warning',
       showCancelButton: true,
       confirmButtonColor: '#3085d6',
       cancelButtonColor: '#d33',
       confirmButtonText: 'Confirm!'
     }).then((result) => {
       if (result.isConfirmed) {
         $.ajax({
                 url: '/admin/userblock/'+userId,
                 method: 'get',
                 success : (response) =>{
                   if(response.status){
                     Swal.fire({
                       icon : 'success',
                       title: 'User Blocked.',
                       showConfirmbutton:false
                   }).then(()=>{
                       location.reload()
                   })
                   }
                 }
               })
       }
     })
        }
       </script>
         <script>
          function deletecategory(category){
          Swal.fire({
          title: 'Are you sure?',
          text: "You are deleting a category!",
          icon: 'warning',
          showCancelButton: true,
          confirmButtonColor: '#3085d6',
          cancelButtonColor: '#d33',
          confirmButtonText: 'Confirm!'
          }).then((result) => {
          if (result.isConfirmed) {
            $.ajax({
                  url: '/admin/deletecategory/'+category,
                  method: 'get',
                  success : (response) =>{
                    if(response.status){
                      Swal.fire({
                        icon : 'success',
                        title: 'Your Category is Deleted.',
                        showConfirmbutton:false
                    }).then(()=>{
                        location.reload()
                    })
                    }else{
                      Swal.fire({
                        icon : 'error',
                        title: 'Category contains products.',
                        showConfirmbutton:false
                    }).then(()=>{
                        location.reload()
                    })
                    }
                  }
                })
        }
      })
          }
          
        </script>
        <script>
          function softdeleteproduct(productId){
            Swal.fire({
          title: 'Are you sure?',
          text: "You are Softdeleting a product!",
          icon: 'warning',
          showCancelButton: true,
          confirmButtonColor: '#3085d6',
          cancelButtonColor: '#d33',
          confirmButtonText: 'Confirm!'
          }).then((result) => {
          if (result.isConfirmed) {
            $.ajax({
                  url: '/admin/softdeleteproduct/'+productId,
                  method: 'get',
                  success : (response) =>{
                    if(response.status){
                      Swal.fire({
                        icon : 'success',
                        title: 'Your file has been Softdeleted.',
                        showConfirmbutton:false
                    }).then(()=>{
                        location.reload()
                    })
                    }
                  }
                })
        }
      })
          }
        </script>
          <script>
            function deleteproduct(productId){
            Swal.fire({
              title: 'Are you sure?',
              text: "You won't be able to revert this!",
              icon: 'warning',
              showCancelButton: true,
              confirmButtonColor: '#3085d6',
              cancelButtonColor: '#d33',
              confirmButtonText: 'Yes, delete it!'
            }).then((result) => {
                if (result.isConfirmed) {
                  $.ajax({
                    url: '/admin/deleteproduct/'+productId,
                    method: 'get',
                    success : (response) =>{
                      if(response.status){
                        Swal.fire({
                          icon : 'Deleted',
                          title: 'Your file has been deleted.',
                          showConfirmbutton:false
                      }).then(()=>{
                          location.reload()
                      })
                      }
                    }
                  })
              }
            })
            }
